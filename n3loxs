    elif variable['type'] == 9 or variable['type'] == 10:
        # Drell-Yan p p(pbar) --> W+ + X --> l+ nu_l + X / W- + X --> l- ~nu_l + X in an invariant mass window between Qmin and Qmax
        if variable['type'] == 9:
