                    filetest0=proctag+'_ppbar_dyn_'+energy2+'tev_pdf'+variable['pdfnum']+'_muf1_mur0.5.txt'
                    filetest2=proctag+'_ppbar_dyn_'+energy2+'tev_pdf'+variable['pdfnum']+'_muf1_mur2.txt'
